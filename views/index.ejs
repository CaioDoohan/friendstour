      <div class="container">
        <div class="row">
          <section class="crl pr">
            <ul class="carhome">
              <%if (banner != undefined){
                for(var k = 0; k < banner.length; k++){%>
                  <li>
                    <div class="item">
                      <figure>
                        <img src="/images/uploads/banners/<%= banner[k]%>" alt="Friend's Tour" />
                      </figure>
                    </div>
                  </li>
              <%}
              } else {%>
                <li>
                  <div class="item">
                    <figure>
                      <img src="/images/car1.jpg" alt="Friend's Tour" />
                    </figure>
                  </div>
                </li>
              <%} %>
            </ul>
          </section>
        </div>
        <div class="row">
          <section class="crl vg prom full">
            <div>
               <h3>Promoções</h3>
            </div>
