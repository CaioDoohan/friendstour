                                                  <i class="fa fa-tags" aria-hidden="true"></i>
                                                  <h4>Excursão</h4>
                                                </li>
                                            <% break;
                                                case 'Transporte' :%>                   
                                                <li class="transp">
                                                  <i class="fa fa-bus" aria-hidden="true"></i>
                                                  <h4>Transporte</h4>
                                                </li>
                                            <% break; 
                                              } %>
                                      <% } %>
                                    </ul>
                                    <div class="name">
                                      <h5><%= produto[i].nome_prod %></h5>
                                    </div>
                                    <div class="desc">
                                      <p><%= produto[i].desc_prod %></p>
                                    </div>
                                    <ul class="tempo">
                                      <li>
                                        <h5>Horário:</h5>
                                        <h6><%= produto[i].horario%></h6>
                                      </li>
                                      <li>
                                        <h5>Dia:</h5>
                                        <h6><%= produto[i].data%></h6>
                                      </li>
                                    </ul>
                                    <ul class="inc">
                                      <%for(var k = 0; k < produto[i].inclusos.length; k++){%>
                                        <li>
                                          <i class="fa fa-check" aria-hidden="true"></i>
                                          <h6><%= produto[i].inclusos[k] %></h6>
                                        </li>
                                      <% } %>
                                    </ul>
                                </div>
                                <div class="down">
                                    <div class="valor">
                                      <div>
                                          <h5>
                                            a partir de:
                                          </h5>
                                          <h6>
                                              R$<%= produto[i].valor_prod %>
                                          </h6>
                                      </div>
                                    </div>
                                    <ul class="fpag">
                                      <li class="parc">
                                        <i class="fa fa-plus" aria-hidden="true"></i>
                                        <p>
                                          PAGUE EM ATÉ <span><%= produto[i].parcelas_prod %></span>X !
                                        </p>
                                      </li>
                                    </ul>
                                </div>
                              </div>
                              <%if ( produto[i].vagas_prod == true ) { %>
                                <div class="event ult">
                                  <span>
                                    ÚLTIMAS VAGAS
                                  </span>
                                  <i class="far fa-clock"></i>
                                </div>
                              <% }  else {}%>
                            </div>
                          </li>           
                      <% } 
                      }
                    } 
                    else { %>
                      <li class="undefined">
                        <p>Novidades em breve</p>
                      </li>
                    <% } %>
                </ul>
            </section>
